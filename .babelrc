{
  "presets": [
    ["next/babel"]
    
  ],
  "plugins": [
    "inline-dotenv",
    [
      "babel-plugin-styled-components",
      {
        "minify": false,
        "ssr": false,
        "displayName" : true,
        "preprocess" : false
      }
    ],
    ["module-resolver", {
      "root": ["./src"]
    }]
  ],
  
  "env": {
    "test": {
      "presets": [
        ["next/babel", {
           "preset-env": {
              "modules": "commonjs"
            }
        }]
      ]
    }
  }
}